{
  "name": "trenity-core",
  "version": "1.0.0",
  "scripts": {
    "setup": "yarn setup-server && yarn setup-client",
    "setup-server": "cd server && yarn",
    "setup-client": "cd client && yarn",
    "server": "cd server && node index.js",
    "dev-server": "cd server && yarn sf",
    "client": "cd client && yarn start",
    "build-client": "cd client && yarn build",
    "dev": "concurrently --kill-others-on-fail \"yarn dev-server\" \"yarn client\""
  },
  "devDependencies": {
    "concurrently": "^3.6.0"
  },
  "license": "ISC",
  "dependencies": {
    "react-jss": "^8.6.1"
  }
}
